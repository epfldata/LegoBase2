AGGSUM([LINEITEM_RETURNFLAG, LINEITEM_LINESTATUS], (LINEITEM(LINEITEM_ORDERKEY, LINEITEM_PARTKEY, LINEITEM_SUPPKEY, LINEITEM_LINENUMBER, LINEITEM_QUANTITY, LINEITEM_EXTENDEDPRICE, LINEITEM_DISCOUNT, LINEITEM_TAX, LINEITEM_RETURNFLAG, LINEITEM_LINESTATUS, LINEITEM_SHIPDATE, LINEITEM_COMMITDATE, LINEITEM_RECEIPTDATE, LINEITEM_SHIPINSTRUCT, LINEITEM_SHIPMODE, LINEITEM_COMMENT) * {LINEITEM_SHIPDATE <= DATE '1997-9-1'} * {LINEITEM_EXTENDEDPRICE} * ({1} + ({-1.0} * {LINEITEM_DISCOUNT}))))
